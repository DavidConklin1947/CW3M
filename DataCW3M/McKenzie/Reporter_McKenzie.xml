  <output_group name="REP Wetlands" >
    <output name="Number of Wetland IDUs" query="LULC_A=6" value="1" type="sum" />
    <output name="total area of wetlands (ha)" query="LULC_A=6" value="AREA/10000" type="sum" />
    <output name="population of wetlands" query="LULC_A=6" value="POP" type="sum" />
  </output_group>

  <output_group name="REP climate" >
    <output name="PRECIP_YR (mm H2O) total precipitation" query="1" value="PRECIP_YR" type="areaWtMean" />
    <output name="TEMP_YR (degC) average daily temperature" query="1" value="TEMP" type="areaWtMean" />
  </output_group>
  <output_group name="REP Urban Water Total Demand" >
    <output name="Total urban residential demand (ccf/day)" query="UGB>0" value="H2ORESIDNT" type="sum" />
    <output name="Total industrial/commercial demand (ccf/day)" query="UGB=40 {Metro} or UGB=12 {Corvallis} or UGB=22 {Eugene/Springfield} or UGB=51 {Salem/Keiser}" value="H2OINDCOMM" type="sum" />
    <output name="Total rural residential demand (ccf/day)" query="UGB=0" value="H2ORESIDNT" type="sum" />
    <output name="Total Water Demand (ccf/day)" query="Area &gt; 0" value="H2ORESIDNT+H2OINDCOMM" type="sum" />
  </output_group>

  <output_group name="REP Quick Check (mm H2O)">
    <output name="Precip (mm H2O)" query="1" value ="PRECIP_YR" type="areaWtMean"/>
    <output name="GW pumping (mm H2O)"  query="1" value="((IRGWAF_Y*m3PerAcreFt+GWMUNALL_Y)/AREA)*1000." type="areaWtMean"  />
    <output name="AET (mm H2O)" query="1" value="ET_YR" type="areaWtMean" />
    <output name="SNOWEVAP_Y (mm H2O)" query="1" value="SNOWEVAP_Y" type="areaWtMean" />
    <output name="RAINEVAP_Y (mm H2O)" query="1" value="RAINEVAP_Y" type="areaWtMean" />
    <output name="Runoff as Precip plus GW minus AET minus RAINEVAP minus SNOWEVAP (mm H2O)"  query="1" value="PRECIP_YR + ((IRGWAF_Y*m3PerAcreFt + GWMUNALL_Y)/AREA)*1000. - ET_YR - SNOWEVAP_Y - RAINEVAP_Y" type="areaWtMean"  />
    <output name="ET_YR in forest (mm H2O)" query="LULC_A=4 and PVT>0" value="ET_YR" type="areaWtMean" />
    <output name="SNOWEVAP_Y in forest (mm H2O)" query="LULC_A=4 and PVT>0" value="SNOWEVAP_Y" type="areaWtMean" />
    <output name="RAINEVAP_Y in forest (mm H2O)" query="LULC_A=4 and PVT>0" value="RAINEVAP_Y" type="areaWtMean" />
    <output name="ET_YR in ag (mm H2O)" query="LULC_A=2" value="ET_YR" type="areaWtMean" />
    <output name="SNOWEVAP_Y in ag (mm H2O)" query="LULC_A=2" value="SNOWEVAP_Y" type="areaWtMean" />
    <output name="RAINEVAP_Y in ag (mm H2O)" query="LULC_A=2" value="RAINEVAP_Y" type="areaWtMean" />
    <output name="MAX_ET_YR in forest (mm H2O)" query="LULC_A=4 and PVT>0" value="MAX_ET_YR" type="areaWtMean" />
    <output name="MAX_ET_YR in ag (mm H2O)" query="LULC_A=2" value="MAX_ET_YR" type="areaWtMean" />
  </output_group>
  
  <output_group name="REP Quick Check (inches H2O ave over irrigated area)">
    <output name="AET (inches H2O ave over irrigated area)" query="IRRIGATION=1" value="ET_YR*inches_per_mm" type="areaWtMean" />
    <output name="Precip (inches H2O ave over irrigated area)" query="IRRIGATION=1" value ="PRECIP_YR*inches_per_mm" type="areaWtMean"/>
    <output  name="Irrigation (inches H2O ave over irrigated area)"  query="IRRIGATION=1" value="(IRRALAF_Y*m3PerAcreFt/AREA)*1000.*inches_per_mm" type="areaWtMean"  />
    <output  name="Irrigation from GW (inches H2O ave over irrigated area)"  query="IRRIGATION=1" value="(IRGWAF_Y*m3PerAcreFt/AREA)*1000.*inches_per_mm" type="areaWtMean"  />
  </output_group>

  <output_group name="REP Quick Check (ac-ft H2O)">
    <output  name="Irrigation (ac-ft H2O)"  query="1" value="IRRALAF_Y" type="sum"  />
    <output  name="Irrigation from SW (ac-ft H2O)"  query="1" value="IRSWAF_Y" type="sum"  />
    <output  name="Irrigation from GW (ac-ft H2O)"  query="1" value="IRGWAF_Y" type="sum"  />
    <output  name="Urban water from SW (ac-ft H2O)"  query="1" value="SWMUNALL_Y/m3PerAcreFt" type="sum"  />
    <output  name="Urban water from GW (ac-ft H2O)"  query="1" value="GWMUNALL_Y/m3PerAcreFt" type="sum"  />
  </output_group>
  
  <output_group name="REP Quick Check (ccf H2O per day)">
    <output name="H2ORESIDNT demand (ccf H2O per day)" query="1" value="H2ORESIDNT" type="sum" />
    <output name="H2OINDCOMM demand (ccf H2O per day)" query="1" value="H2OINDCOMM" type="sum" />
    <output  name="GAL_CAP_DYxPOP (ccf H2O per day)"  query="1" value="GAL_CAP_DY*POP/gal_per_ccf" type="sum"  />
    <output  name="Rural domestic H2O demand without WR (ccf H2O per day)"  query="POP>0 and UGB=0 and WR_MUNI=0" value="H2ORESIDNT" type="sum"  />
    <output name="Rural domestic H2O supplied without WR (ccf H2O per day)" query="POP>0 and UGB=0 and WR_MUNI=0" value="GAL_CAP_DY*POP/gal_per_ccf" type="sum" />
  </output_group>

  <output_group name="REP Irrigation (number of IDUs)">
    <output  name="Groundwater irrigation water rights WR_IRRIG_G=1 (number of IDUs)"  query="WR_IRRIG_G=1" value="WR_IRRIG_G" type="sum"  />
    <output  name="Surface water irrigation water rights WR_IRRIG_S=1 (number of IDUs)"  query="WR_IRRIG_S=1" value="WR_IRRIG_S" type="sum"  />
    <output  name="Decision to irrigate IRRIGATION=1 (number of IDUs)"  query="IRRIGATION=1" value="IRRIGATION" type="sum"  />
    <output  name="Transient irrigation water shortages WR_SHORTG=1 (number of IDUs)"  query="WR_SHORTG=1" value="WR_SHORTG" type="sum"  />
    <output  name="Persistent irrigation water shortages WR_SHORTG=2 (number of IDUs)"  query="WR_SHORTG=2" value="WR_SHORTG/2" type="sum"  />
    <output  name="Water right shutoffs WR_SHUTOFF=1 (number of IDUs)"  query="1" value="WR_SHUTOFF" type="sum"  />
  </output_group>

  <output_group name="REP Basin Population" >
    <output name="Population in UGAs" query="UGB>0" value="POP" type="sum" />
    <output name="Rural population in RPAs" query="UGB&lt;=0 and RPA>0" value="POP" type="sum" />
    <output name="Rural population outside of RPAs" query="UGB&lt;=0 and RPA&lt;=0" value="POP" type="sum" />
    <output name="Total population" query="1" value="POP" type="sum" />
  </output_group>

  <output_group name="REP Forest Harvest-Fire-Disequilibrium (ac)">
    <output name="Wildfire (ac)" query="DISTURB=23 {Stand-replacing wildfire}" value="AREA/m2perAcre" type="sum" />
    <output name="Timber harvest (ac)" query="DISTURB=1 {Harvest} and LULC_A=4 and PVT>0" value="AREA/m2perAcre" type="sum" />
    <output name="Timber harvest from all public lands (ac)" query="DISTURB=1 {Harvest} and LULC_A=4 and PVT>0 and (OWNER=2 {Federal} or OWNER=3 {State} or OWNER=4 {County})" value="AREA/m2perAcre" type="sum" />
    <output name="Timber harvest from protected public lands (ac)" query="DISTURB=1 {Harvest} and LULC_A=4 and PVT>0 and (OWNER=2 {Federal} or OWNER=3 {State} or OWNER=4 {County}) and CONSERVE>0" value="AREA/m2perAcre" type="sum" />
    <output name="Timber harvest from unprotected public lands (ac)" query="DISTURB=1 {Harvest} and LULC_A=4 and PVT>0 and (OWNER=2 {Federal} or OWNER=3 {State} or OWNER=4 {County}) and CONSERVE=0" value="AREA/m2perAcre" type="sum" />
    <output name="Timber harvest from private lands (ac)" query="DISTURB=1 {Harvest} and LULC_A=4 and PVT>0 and (OWNER=0 {private, non-industrial} or OWNER=1 {private industrial forest})" value="AREA/m2perAcre" type="sum" />
    <output name="Private timber >= 40 yrs (ac)" query="AGECLASS >= 40 and LULC_A=4 and PVT>0 and (OWNER=0 {private, non-industrial} or OWNER=1 {private industrial forest})" value="AREA/m2perAcre" type="sum" />
    <output name="All public timber >= 40 and &lt;= 80 yrs (ac)" query="AGECLASS >= 40 and AGECLASS &lt;= 80 and LULC_A=4 and PVT>0 and (OWNER=2 {federal} or OWNER=3 {state} or OWNER=4 {county})" value="AREA/m2perAcre" type="sum" />
    <output name="Protected public timber >= 40 and &lt;= 80 yrs (ac)" query="AGECLASS >= 40 and AGECLASS &lt;= 80 and LULC_A=4 and PVT>0 and (OWNER=2 {federal} or OWNER=3 {state} or OWNER=4 {county}) and CONSERVE>0" value="AREA/m2perAcre" type="sum" />
    <output name="Unprotected public timber >= 40 and &lt;= 80 yrs (ac)" query="AGECLASS >= 40 and AGECLASS &lt;= 80 and LULC_A=4  and PVT>0 and (OWNER=2 {federal} or OWNER=3 {state} or OWNER=4 {county}) and CONSERVE=0" value="AREA/m2perAcre" type="sum" />
    <output name="Unprotected public timber >= 40 ayrs (ac)" query="AGECLASS >= 40 and LULC_A=4 and PVT>0 and (OWNER=2 {federal} or OWNER=3 {state} or OWNER=4 {county}) and CONSERVE=0" value="AREA/m2perAcre" type="sum" />
    <output name="Forest disequilibrium with climate (ac)" query="LULC_A=4 and PVT>0 and DISTURB>100 and DISTURB&lt;200" value="AREA/m2perAcre" type="sum" />
  </output_group>

  <!-- Dave Turner's forest water flux outputs -->
  <output_group name="REP Mean annual temperature (deg C)">
    <output name="Mean annual temperature of whole basin (TEMP_YR deg C)" query="1" value="TEMP_YR" type="AREAWTMEAN" />
    <output name="Mean annual temperature of forests (TEMP_YR deg C)" query="LULC_A=4 and PVT>0" value="TEMP_YR" type="AREAWTMEAN" />
  </output_group>
  <output_group name="REP Forest ET and Precip (mm H2O)">
    <output name="AET (mm)" query="LULC_A=4 and PVT>0" value="ET_yr" type="AREAWTMEAN" />
    <output name="Maximum ET (mm)" query="LULC_A=4 and PVT>0" value="MAX_ET_yr" type="AREAWTMEAN" />
    <output name="Precip (mm)" query="LULC_A=4 and PVT>0" value="Precip_yr" type="AREAWTMEAN" />
    <output name="RAINTHRU_Y (mm H2O)" query="LULC_A=4 and PVT>0" value="RAINTHRU_Y" type="areaWtMean" />
    <output name="RAINEVAP_Y (mm H2O)" query="LULC_A=4 and PVT>0" value="RAINEVAP_Y" type="areaWtMean" />
    <output name="SNOWTHRU_Y (mm H2O)" query="LULC_A=4 and PVT>0" value="SNOWTHRU_Y" type="areaWtMean" />
    <output name="SNOWEVAP_Y (mm H2O)" query="LULC_A=4 and PVT>0" value="SNOWEVAP_Y" type="areaWtMean" />
    <output name="TET (mm H2O)" query="LULC_A=4 and PVT>0" value="ET_yr + RAINEVAP_Y + SNOWEVAP_Y" type="areaWtMean" />
  </output_group>

  <output_group name="REP McKenzie Basin Forest Snow etc (mm H2O)">
    <output name="AET (mm)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="ET_yr" type="AREAWTMEAN" />
    <output name="Maximum ET (mm)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="MAX_ET_yr" type="AREAWTMEAN" />
    <output name="Precip (mm)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="Precip_yr" type="AREAWTMEAN" />
    <output name="RAINTHRU_Y (mm H2O)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="RAINTHRU_Y" type="areaWtMean" />
    <output name="RAINEVAP_Y (mm H2O)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="RAINEVAP_Y" type="areaWtMean" />
    <output name="SNOWTHRU_Y (mm H2O)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="SNOWTHRU_Y" type="areaWtMean" />
    <output name="SNOWEVAP_Y (mm H2O)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="SNOWEVAP_Y" type="areaWtMean" />
    <output name="Apr 1 SWE (mm)" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="SNOW_APR" type="AREAWTMEAN" />
    <output name="LAI" query="LULC_A=4 and PVT>0 and SUB_AREA=1" value="LAI" type="AREAWTMEAN" />
  </output_group>
  
  <output_group name="REP LAI (m2/m2)">
   <output name="LAI" query="LULC_A=4 and PVT>0" value="LAI" type="AREAWTMEAN" />
    <output name="LAI on public  lands" query="LULC_A=4 and PVT>0 and (OWNER=2 {Federal} or OWNER=3 {State} or OWNER=4 {County})" value="LAI" type="AREAWTMEAN" />
    <output name="LAI on private lands" query="LULC_A=4 and PVT>0 and (OWNER=0 {private, non-industrial} or OWNER=1 {private industrial forest})" value="LAI" type="AREAWTMEAN" />    
  </output_group>

  <!-- Bill Jaeger's Econ metrics -->
  <output_group name="REP Irrigable Ag Land (ac)">
    <output name="Mckenzie all ag land (ac)" query="SUB_AREA=1 and LULC_A=2" value="AREA*ac_per_m2" type="SUM" />
    <output name="Mckenzie ag land irrigable from surface water (ac)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_S=1" value="AREA*ac_per_m2" type="SUM" />
    <output name="Mckenzie ag land irrigable from ground water (ac)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_G=1" value="AREA*ac_per_m2" type="SUM" />
  </output_group>
  
  <output_group name="REP Irrigation requested (ac-ft)">
    <output name="Mckenzie ag land irrigation requested from surface water (ac-ft)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_S=1" value="IRRRQST_Y" type="SUM" />
    <output name="Mckenzie ag land irrigation requested from ground water (ac-ft)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_G=1" value="IRRRQST_Y" type="SUM" />
  </output_group>
  
  <output_group name="REP Irrigation allocated (ac-ft)">
    <output name="Mckenzie ag land irrigation allocated from surface water (ac-ft)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_S=1" value="IRRALAF_Y" type="SUM" />
    <output name="Mckenzie ag land irrigation allocated from ground water (ac-ft)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_G=1" value="IRRALAF_Y" type="SUM" />
  </output_group>
  
  <output_group name="REP Unexercised water rights (ac)">
     <output name="Mckenzie ag land unexercised surface water rights (ac)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_S=1 and IRRIGATION=0" value="AREA*ac_per_m2" type="SUM" />
    <output name="Mckenzie ag land unexercised groundwater rights (ac)" query="SUB_AREA=1 and LULC_A=2 and WR_IRRIG_G=1 and IRRIGATION=0" value="AREA*ac_per_m2" type="SUM" />
   </output_group>

 
  <output_group name="REP New Irrigation Water Rights" >
    <output name="Cumulative count of new water rights" query="WR_PROB &lt; 0" value="1" type="sum" />
    <output name="Approx average WR_PROB of new water rights" query="WR_PROB &lt; 0" value="-WR_PROB" type="AREAWTMEAN" />
    <output name="Approx average IRP of new water rights ($ per ac)" query="WR_PROB &lt; 0" value="IRP" type="AREAWTMEAN" />
    <output name="Approx average ECC of new water rights ($ per ac)" query="WR_PROB &lt; 0" value="ECC" type="AREAWTMEAN" />
    <output name="Area of new water rights (ac)" query="WR_PROB &lt; 0" value="AREA/m2perAcre" type="sum" />
    <output name="Count of remaining potential new water rights" query="WR_PROB &gt; 0" value="1" type="sum" />
    <output name="Approx average WR_PROB of potential water rights" query="WR_PROB &gt; 0" value="WR_PROB" type="AREAWTMEAN" />
    <output name="Area of potential water rights (ac)" query="WR_PROB &gt; 0" value="AREA/m2perAcre" type="sum" />
  </output_group>

     <output_group name="REP Irrigation Rent Premium Metrics" >
       <output name="IRP for McKenzie basin ($)" query="SUB_AREA=1 and LULC_A=2 and (WR_IRRIG_S=1 or WR_IRRIG_G=1)" value="IRP*(AREA/m2perAcre)" type="SUM" />
       <output name="IRP loss due to WR shutoffs for McKenzie basin ($)" query="SUB_AREA=1 and LULC_A=2 and (WR_IRRIG_S=1 or WR_IRRIG_G=1) and WR_SHUTOFF!=0" value="IRP*(AREA/m2perAcre)" type="SUM" />
     </output_group>
     <!-- Metrics for irrigation analysis -->
<!--
    <output_group name="REP IDU 90470 Irrigation requested and allocated (ac-ft)">
      <output name="IDU 90470 irrigation requested from surface water (ac-ft)" query="LULC_A=2 and WR_IRRIG_S=1 and IDU_INDEX=90470" value="IRRRQST_Y" type="SUM" />
      <output name="IDU 90470 irrigation requested from ground water (ac-ft)" query="LULC_A=2 and WR_IRRIG_G=1 and IDU_INDEX=90470" value="IRRRQST_Y" type="SUM" />
      <output name="IDU 90470 irrigation allocated from surface water (ac-ft)" query="LULC_A=2 and WR_IRRIG_S=1 and IDU_INDEX=90470" value="IRRRQST_Y" type="SUM" />
      <output name="IDU 90470 irrigation allocated from ground water (ac-ft)" query="LULC_A=2 and WR_IRRIG_G=1 and IDU_INDEX=90470" value="IRRRQST_Y" type="SUM" />
