<?xml version="1.0" encoding="utf-8"?>


<reporter>
  <const name='m2perAcre' value='4046.86' />
  <const name="ac_per_m2" value="0.000247105" />
  <const name="mPermm" value="0.001" />
  <const name="m3PerAcreFt" value ="1233.48" />
  <const name="sec_per_day" value="86400" />
  <const name="ccf_per_m3" value="0.35315" />
  <const name="gal_per_ccf" value="748.052" />
  <const name="inches_per_mm" value="0.039370" />
  <const name="ft_per_m" value="3.28084" />

  <output_group name="REP Forest Disturbance Report"  >
    <output name="Forest area (ha)" query="LULC_A=4" value="AREA/10000" type="sum"  />
    <output name="Forest area above 1000 m (ha)" query="LULC_A=4 and ELEV_MEAN>1000" value="AREA/10000" type="sum"  />
    <output name="Forest harvest area (ha)" query="LULC_A=4 and DISTURB=1" value="AREA/10000" type="sum"  />
    <output name="Forest harvest area above 1000 m (ha)" query="LULC_A=4 and DISTURB=1 and ELEV_MEAN>1000" value="AREA/10000" type="sum"  />
    <output name="Forest stand-replacing fire area (ha)" query="LULC_A=4 and DISTURB=23" value="AREA/10000" type="sum" />
    <output name="LAI" query="LULC_A=4" value="LAI" type="areaWtMean"  />
    <output name="LAI above 1000 m" query="LULC_A=4 and ELEV_MEAN>1000" value="LAI" type="areaWtMean"  />
    <output name="Yearly ET (mmH2O)" query="LULC_A=4" value="ET_DAY*365" type="areaWtMean"  />
    <output name="Yearly ET above 1000 m (mmH2O)" query="LULC_A=4 and ELEV_MEAN > 1000" value="ET_DAY*365" type="areaWtMean"  />
  </output_group>

  <output_group name="REP Mohawk25 water budget" >
    <output name="ET_yr (mm H2O) Mohawk25" query="HBVCALIB=25" value="GET_yr" type="areaWtMean" />
    <output name="GWMUNALL_Y (cms) Mohawk25" query="HBVCALIB=25" value="GWMUNALL_Y" type="sum" />
    <output name="SWMUNALL_Y (cms) Mohawk25" query="HBVCALIB=25" value="SWMUNALL_Y" type="sum" />
    <output name="IRRALAF_Y (ac-ft) Mohawk25" query="HBVCALIB=25" value="IRRALAF_Y" type="sum" />
    <output name="IRRALLO_Y (mm H2O) Mohawk25" query="HBVCALIB=25" value="IRRALLO_Y" type="sum" />
  </output_group>
<!--
  <output_group name="REP McKenzie wetland elevations (m ASL)" >
    <output name="WETL 1 ELEV_MEAN McKenzie R outlet 113m elev" query="WETL_ID=1" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 2 ELEV_MEAN McKenzie R 116m elev" query="WETL_ID=2" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 3 ELEV_MEAN 118m elev" query="WETL_ID=3" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 4 ELEV_MEAN McKenzie R 120m elev" query="WETL_ID=4" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 5 ELEV_MEAN McKenzie R 124m elev" query="WETL_ID=5" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 6 ELEV_MEAN McKenzie R 123m elev" query="WETL_ID=6" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 7 ELEV_MEAN McKenzie R 125m elev" query="WETL_ID=7" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 8 ELEV_MEAN 129m elev" query="WETL_ID=8" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 9 ELEV_MEAN McKenzie R 129m elev" query="WETL_ID=9" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 10 ELEV_MEAN McKenzie R 132m elev" query="WETL_ID=10" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 11 ELEV_MEAN McKenzie R 131m elev" query="WETL_ID=11" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 12 ELEV_MEAN McKenzie R 134m elev" query="WETL_ID=12" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 13 ELEV_MEAN McKenzie R 135m elev" query="WETL_ID=13" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 14 ELEV_MEAN McKenzie R 139m elev" query="WETL_ID=14" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 15 ELEV_MEAN McKenzie R 162m elev" query="WETL_ID=15" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 16 ELEV_MEAN Cedar Cr 149m elev" query="WETL_ID=16"  value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 17 ELEV_MEAN Cedar Cr 162m elev" query="WETL_ID=17" value="ELEV_MEAN" type="areaWtMean" />
    <output name="WETL 18 ELEV_MEAN McGowan Cr 539m elev" query="WETL_ID=18" value="ELEV_MEAN" type="areaWtMean" />
