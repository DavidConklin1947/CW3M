    obs="..\Observations\Willamette Gages\Flow\USGS 14180300_flow_BLOWOUT CREEK NEAR DETROIT, OR_23780557.csv" format="E0" />
  </output_group>

  <output_group name="FLOW ET by watershed and reservoir" interval="yearly" >
    <output in_use="1" name="ET from Blowout51 avg mm/day" query="HBVCALIB=51" value="ET_DAY" type="areaWtMean" domain="idu" />
    <output in_use="1" name="ET from Blowout51 avg m3/day" query="HBVCALIB=51" value="ET_DAY*Area/1000" type="sum" domain="idu" />
    <output in_use="1" name="ET from Blowout51 avg cfs" query="HBVCALIB=51" value="(ET_DAY*Area/1000)/86400" type="sum" domain="idu" />
  </output_group>

  <output_group name="FLOW Blowout51 water budget" interval="yearly" >
    <output name="PRECIP (mm H2O)" query="HBVCALIB=51" value="PRECIP" type="areaWtMean" domain="idu" />
    <output name="SNOW_EVAP (mm SWE)" query="HBVCALIB=51" value="SNOW_EVAP" type="areaWtMean" domain="idu" />
    <output in_use="1" name="ET from Blowout51 mm" query="HBVCALIB=51" value="ET_DAY" type="areaWtMean" domain="idu" />
    <output name="SPRING_CMS" query="HBVCALIB=51" value="SPRING_CMS" type="sum" domain="reach" />
    <output name="ADDED_VOL (m3)" query="HBVCALIB=51" value="ADDED_VOL" type="sum" domain="reach" />
    <output in_use="1" name="adjusted Q (cfs)" query="COMID=23780557" value="Q_DISCHARG*35.315*1.012" type="sum" domain="reach"
    obs="..\Observations\NSantiam\USGS_14180300_flow_BLOWOUT CREEK NEAR DETROIT, OR_23780557.csv" format="E0" />
  </output_group>
