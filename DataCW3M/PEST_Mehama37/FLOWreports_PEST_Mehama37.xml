<?xml version="1.0" encoding="utf-8"?>


<outputs>
  <output_group name="FLOW Q_DISCHARG at gage daily" interval="daily" >
    <output in_use="1" name="adjusted Q_DISCHARG (cfs)" query="COMID=23780481" value="Q_DISCHARG*35.315*0.996" type="sum" domain="reach"
    obs="..\Observations\NSantiam\USGS_14183000_flow_NORTH SANTIAM RIVER AT MEHAMA, OR_23780481.csv" format="E0" />
  </output_group>

  <output_group name="FLOW Q_DISCHARG at gage monthly" interval="monthly" >
    <output in_use="1" name="adjusted Q_DISCHARG (cfs)" query="COMID=23780481" value="Q_DISCHARG*35.315*0.996" type="sum" domain="reach"
    obs="..\Observations\NSantiam\USGS_14183000_flow_NORTH SANTIAM RIVER AT MEHAMA, OR_23780481.csv" format="E0" />
    <output in_use="1" name="USGS_14178000_flow_NO SANTIAM R BLW BOULDER CRK, NR DETROIT, OR_23780591"
            query="COMID=23780591" value="Q_DISCHARG*35.3146667*1.003" type="sum" domain="reach"
           obs="..\Observations\NSantiam\USGS_14178000_flow_NO SANTIAM R BLW BOULDER CRK, NR DETROIT, OR_23780591.csv" format="E0" />
    <output in_use="1" name="USGS_14179000_flow_BREITENBUSH R ABV FRENCH CR NR DETROIT, OR_237807011"
            query="COMID=23780701" value="Q_DISCHARG*35.3146667*1.024" type="sum" domain="reach"
           obs="..\Observations\NSantiam\USGS_14179000_flow_BREITENBUSH R ABV FRENCH CR NR DETROIT, OR_23780701.csv" format="E0" />
    <output in_use="1" name="USGS_14180300_flow_BLOWOUT CREEK NEAR DETROIT, OR_23780557"
            query="COMID=23780557" value="Q_DISCHARG*35.3146667*1.012" type="sum" domain="reach"
           obs="..\Observations\NSantiam\USGS_14180300_flow_BLOWOUT CREEK NEAR DETROIT, OR_23780557.csv" format="E0" />
    <output in_use="1" name="USGS_14181500_flow_NORTH SANTIAM RIVER AT NIAGARA, OR_23780511"
            query="COMID=23780511" value="Q_DISCHARG*35.3146667*1.034" type="sum" domain="reach"
           obs="..\Observations\NSantiam\USGS_14181500_flow_NORTH SANTIAM RIVER AT NIAGARA, OR_23780511.csv" format="E0" />
    <output in_use="1" name="USGS_14182500_flow_LITTLE NORTH SANTIAM RIVER NEAR MEHAMA, OR_23780805"
            query="COMID=23780805" value="Q_DISCHARG*35.3146667*0.993" type="sum" domain="reach"
           obs="..\Observations\NSantiam\USGS_14182500_flow_LITTLE NORTH SANTIAM RIVER NEAR MEHAMA, OR_23780805.csv" format="E0" />
  </output_group>

  <output_group name="FLOW ET by watershed and reservoir" interval="yearly" >
    <output in_use="1" name="ET from Mehama37 avg mm/day" query="HBVCALIB=37" value="ET_DAY" type="areaWtMean" domain="idu" />
    <output in_use="1" name="ET from Mehama37 avg m3/day" query="HBVCALIB=37" value="ET_DAY*Area/1000" type="sum" domain="idu" />
    <output in_use="1" name="ET from Mehama37 avg cfs" query="HBVCALIB=37" value="(ET_DAY*Area/1000)/86400" type="sum" domain="idu" />
  </output_group>

  <output_group name="FLOW Mehama37 water budget" interval="yearly" >
    <output name="PRECIP (mm H2O)" query="HBVCALIB=37" value="PRECIP" type="areaWtMean" domain="idu" />
    <output name="SNOW_EVAP (mm SWE)" query="HBVCALIB=37" value="SNOW_EVAP" type="areaWtMean" domain="idu" />
    <output in_use="1" name="ET from Mehama37 mm" query="HBVCALIB=37" value="ET_DAY" type="areaWtMean" domain="idu" />
    <output name="SPRING_CMS" query="HBVCALIB=37" value="SPRING_CMS" type="sum" domain="reach" />
    <output name="ADDED_VOL (m3)" query="HBVCALIB=37" value="ADDED_VOL" type="sum" domain="reach" />
    <output in_use="1" name="adjusted Q_DISCHARG (cfs)" query="COMID=23780481" value="Q_DISCHARG*35.315*0.996" type="sum" domain="reach"
    obs="..\Observations\NSantiam\USGS_14183000_flow_NORTH SANTIAM RIVER AT MEHAMA, OR_23780481.csv" format="E0" />
  </output_group>
